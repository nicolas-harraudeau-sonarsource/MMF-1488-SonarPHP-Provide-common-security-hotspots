    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "require": {
        "cakephp/cakephp": "^3.6",
        "zendframework/zendframework": "^3.0",
        "illuminate/encryption": "^5.7",
        "illuminate/http": "^5.7",
        "laravel/framework": "^5.7",
        "phpseclib/phpseclib": "^2.0",
        "defuse/php-encryption": "^2.2",
        "paragonie/sodium_compat": "^1.7",
        "codeigniter/framework": "^3.1",
        "craftcms/cms": "^3.0",
        "drupal/encrypt": "~2.3",
        "joomla/crypt": "^1.4",
        "joomla/application": "^1.9",
        "joomla/http": "^1.3",
        "zendframework/zend-console": "^2.7",
        "ulrichsg/getopt-php": "^3.0"
    },
    "config": {
        "vendor-dir": "vendor/"
    },
    "scripts": {
        "run": "App\\Application::main"
    }
