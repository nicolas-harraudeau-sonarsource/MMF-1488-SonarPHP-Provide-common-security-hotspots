{
    "name": "nicolasharraudeau/base_app",
    "type": "project",
    "require-dev": {
        "pds/skeleton": "^1.0",
        "phpstan/phpstan": "^0.10.5",
        "phan/phan": "^1.1"
    },
    "authors": [
        {
            "name": "Nicolas Harraudeau",
            "email": "nicolas.harraudeau@sonarsource.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/7"
        }
